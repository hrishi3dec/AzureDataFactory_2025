{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-covid-reporting-dev-patgiri-001"
		},
		"LS_ABLB_stcovidreportingdev001_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_ABLB_stcovidreportingdev001'"
		},
		"LS_ADLS_dlscovidreportingdev001_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLS_dlscovidreportingdev001'"
		},
		"LS_Databricks_covid_reporting_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'LS_Databricks_covid_reporting'"
		},
		"LS_sql_covid_db_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_sql_covid_db'"
		},
		"LS_ADLS_dlscovidreportingdev001_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlscovidreportingdev001.dfs.core.windows.net/"
		},
		"LS_http_open_data_ecdc_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().baseURL}"
		},
		"LS_sql_covid_db_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "covid-reporting-svr-patgiri.database.windows.net"
		},
		"LS_sql_covid_db_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "covid-db"
		},
		"LS_sql_covid_db_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adm"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-covid-reporting-dev-patgiri-001_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ABLB_stcovidreportingdev001_connectionString": {
						"value": "[parameters('LS_ABLB_stcovidreportingdev001_connectionString')]"
					},
					"LS_ADLS_dlscovidreportingdev001_accountKey": {
						"value": "[parameters('LS_ADLS_dlscovidreportingdev001_accountKey')]"
					},
					"LS_Databricks_covid_reporting_accessToken": {
						"value": "[parameters('LS_Databricks_covid_reporting_accessToken')]"
					},
					"LS_sql_covid_db_password": {
						"value": "[parameters('LS_sql_covid_db_password')]"
					},
					"LS_ADLS_dlscovidreportingdev001_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_dlscovidreportingdev001_properties_typeProperties_url')]"
					},
					"LS_http_open_data_ecdc_properties_typeProperties_url": {
						"value": "[parameters('LS_http_open_data_ecdc_properties_typeProperties_url')]"
					},
					"LS_sql_covid_db_properties_typeProperties_server": {
						"value": "[parameters('LS_sql_covid_db_properties_typeProperties_server')]"
					},
					"LS_sql_covid_db_properties_typeProperties_database": {
						"value": "[parameters('LS_sql_covid_db_properties_typeProperties_database')]"
					},
					"LS_sql_covid_db_properties_typeProperties_userName": {
						"value": "[parameters('LS_sql_covid_db_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-covid-reporting-dev-patgiri-001_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-covid-reporting-dev-patgiri-001_ArmTemplate_0')]"
			]
		}
	]
}